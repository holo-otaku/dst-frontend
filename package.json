  "devDependencies": {
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.11.27",
    "@types/react": "18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
